<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="nextPersonLabel" text="Label">
         <font>
            <Font size="72.0" />
         </font></Label>
        <Button mnemonicParsing="false" text="Done" onAction="#doneActionHandler">
         <font>
            <Font size="36.0" />
         </font></Button>
    </children>
</VBox>
